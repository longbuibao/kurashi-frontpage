{
  "name": "kurashi-frontpage",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "dotenv -e .env.development -- next dev",
    "build": "npx prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "db:manual-seed": "dotenv -e .env.development -- ts-node prisma/seed.mjs",
    "db:reset": "npx prisma migrate dev -n reset",
    "db:mig": "dotenv -e .env.development -- npx prisma migrate dev --name",
    "db:seed": "npx prisma db seed",
    "db:studio": "npx prisma studio",
    "db:deploy": "prisma migrate deploy",
    "db:rollback": "prisma migrate resolve --rolled-back"
  },
  "dependencies": {
    "@prisma/client": "^5.11.0",
    "@studio-freight/react-lenis": "^0.0.47",
    "@types/bcrypt": "^5.0.2",
    "accept-language": "^3.0.18",
    "bcrypt": "^5.1.1",
    "embla-carousel-autoplay": "^8.2.1",
    "embla-carousel-react": "^8.2.1",
    "i18next": "^23.8.2",
    "i18next-browser-languagedetector": "^7.2.0",
    "i18next-resources-to-backend": "^1.2.0",
    "next": "14.1.0",
    "next-auth": "^5.0.0-beta.22",
    "next-nprogress-bar": "^2.3.11",
    "react": "^18",
    "react-cookie": "^7.0.2",
    "react-dom": "^18",
    "react-i18next": "^14.0.5",
    "react-intersection-observer": "^9.13.1",
    "react-loading-skeleton": "^3.4.0",
    "react-photo-album": "^2.3.1",
    "react-range-slider-input": "^3.0.7",
    "react-spinners": "^0.13.8",
    "react-super-responsive-table": "^6.0.0",
    "react-tabs": "^6.0.2",
    "rsuite-table": "^5.18.2",
    "sharp": "^0.33.5",
    "swr": "^2.2.5",
    "uuid": "^9.0.1",
    "yet-another-react-lightbox": "^3.17.2"
  },
  "devDependencies": {
    "@types/node": "^20.11.26",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/uuid": "^9.0.8",
    "autoprefixer": "^10.0.1",
    "dotenv-cli": "^7.4.1",
    "postcss": "^8",
    "prisma": "^5.11.0",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.2",
    "ts-standard": "^12.0.2",
    "typescript": "^5.4.2"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.mjs"
  }
}