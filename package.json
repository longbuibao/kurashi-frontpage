{
  "name": "kurashi-frontpage",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "dotenv -e .env.development -- next dev",
    "build": "npx prisma generate && rimraf .next && next build",
    "postbuild": "next-sitemap",
    "start": "dotenv -e .env -- next start",
    "lint": "next lint",
    "db:manual-seed": "dotenv -e .env.development -- ts-node prisma/seed.mjs",
    "db:reset": "npx prisma migrate dev -n reset",
    "db:mig": "dotenv -e .env.development -- npx prisma migrate dev --name",
    "db:seed": "dotenv -e .env.development -- npx prisma db seed",
    "db:studio": "npx prisma studio",
    "db:deploy": "dotenv -e .env prisma migrate deploy",
    "db:rollback": "prisma migrate resolve --rolled-back",
    "test": "echo testing kurashi-frontpage app",
    "create-env": "printenv >> .env"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.15.0",
    "@hookform/resolvers": "^3.9.1",
    "@jsdevtools/rehype-toc": "^3.0.2",
