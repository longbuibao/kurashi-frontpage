    "remark": "^15.0.1",
    "remark-gfm": "^4.0.1",
    "remark-html": "^16.0.1",
    "remark-rehype": "^11.1.1",
    "remark-toc": "^9.0.0",
    "sharp": "^0.33.5",
    "slate": "^0.102.0",
    "slate-react": "^0.102.0",
